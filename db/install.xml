<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/electalive/db" VERSION="20070228" COMMENT="XMLDB file for Moodle mod/electalive"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="electalive" COMMENT="Contains eLecta Live meetings scheduled in Moodle">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" COMMENT="id" NEXT="course"/>
		<FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" COMMENT="Course docbook activity belongs to" PREVIOUS="id" NEXT="name"/>        
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false"  COMMENT="name" PREVIOUS="course" NEXT="sessiondescription"/>        
        <FIELD NAME="sessiondescription" TYPE="text" NOTNULL="false" UNSIGNED="false" SEQUENCE="false" PREVIOUS="name" NEXT="roomid" />
        <FIELD NAME="roomid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="sessiondescription" NEXT="meetingtime"/>
        <FIELD NAME="meetingtime" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false"  PREVIOUS="roomid" NEXT="timezone"/>
        <FIELD NAME="timezone" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="false" DEFAULT="0" SEQUENCE="false" PREVIOUS="meetingtime" NEXT="meetingtimeend"/>
        <FIELD NAME="meetingtimeend" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="timezone"/>      
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for electalive"/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>