<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/electalive/db" VERSION="20150722" COMMENT="XMLDB file for Moodle mod/electalive"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="electalive" COMMENT="Contains eLecta Live sessions scheduled in Moodle">
        <FIELDS>
            <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" COMMENT="id"/>
            <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false" COMMENT="Course docbook activity belongs to"/>
            <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" COMMENT="name"/>
            <FIELD NAME="intro" TYPE="text" NOTNULL="false" SEQUENCE="false"/>
            <FIELD NAME="roomid" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
            <FIELD NAME="meetingtime" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            <FIELD NAME="timezone" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            <FIELD NAME="meetingtimeend" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
            <FIELD NAME="earlyopen" TYPE="int" LENGTH="5" NOTNULL="true" SEQUENCE="false" COMMENT="time for opening session ahead of time"/>
            <FIELD NAME="moderatorearlyopen" TYPE="int" LENGTH="5" NOTNULL="true" SEQUENCE="false" COMMENT="time for opening session ahead of time for teachers"/>
        </FIELDS>
        <KEYS>
            <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="Primary key for electalive"/>
        </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>